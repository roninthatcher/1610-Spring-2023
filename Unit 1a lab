using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      string name = "Richard Lewison";
      Console.WriteLine(name);  
      float myNum = 5.75F;
      Console.WriteLine(myNum);
      int myNum1 = 4;
      myNum1 = 32; // myNum1 is now 32
      Console.WriteLine(myNum1);
     }
  }
}

using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      int x = 100 + 12;
      Console.WriteLine(x);  
      int y = 30;
      y *= 2;
      Console.WriteLine(y);
      int z = x - y;
      Console.WriteLine(z);
      int q = 9/3;
      q ^= 6;
      Console.WriteLine(q);
     }
  }
}